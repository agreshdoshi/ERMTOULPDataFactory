{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ERMLPTOUDataFactory"
		},
		"LPTOUBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LPTOUBlobStorage'"
		},
		"LPTOUSqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LPTOUSqlDB'"
		},
		"TriggerLPTOU_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/93493f2e-498c-4356-bbe8-2282407a6e98/resourceGroups/ERM/providers/Microsoft.Storage/storageAccounts/ermlptoustorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LPTOUBlobStorage_connectionString": {
						"value": "[parameters('LPTOUBlobStorage_connectionString')]"
					},
					"LPTOUSqlDB_connectionString": {
						"value": "[parameters('LPTOUSqlDB_connectionString')]"
					},
					"TriggerLPTOU_properties_typeProperties_scope": {
						"value": "[parameters('TriggerLPTOU_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}